{
  "name": "weeb_bot",
  "version": "1.1.0",
  "description": "Notify on new manga from Mangadex",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "node built/main.js",
    "fstart": "pm2 start --name weebbot built/main.js",
    "watch": "nodemon --watch src/** --ext ts,json --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/main.ts",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "bot-framework": "Mxblu/bot-framework#8.0.1",
    "bufferutil": "4.0.3",
    "discord-api-types": "0.37.103",
    "discord.js": "14.16.3",
    "dotenv": "8.0.0",
    "fuse.js": "6.4.6",
    "ioredis": "4.17.3",
    "jsdom": "19.0.0",
    "mangadex-full-api": "6.0.0",
    "utf-8-validate": "5.0.5"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.13",
    "@types/node": "20.10.0",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "eslint": "7.28.0",
    "nodemon": "^2.0.9",
    "ts-node": "10.9.1",
    "typescript": "4.3.5"
  }
}
