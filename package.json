{
  "name": "weeb_bot",
  "version": "0.3.0",
  "description": "Notify on new manga from Mangadex",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --watch src/** --ext ts,json --exec node --experimental-specifier-resolution=node --loader ts-node/esm src/main.ts",
    "fstart": "pm2 start --name weebbot built/main.js",
    "build": "tsc --build",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "bot-framework": "Mxblu/bot-framework#command-handler",
    "bufferutil": "4.0.2",
    "discord.js": "12.5.3",
    "dotenv": "8.0.0",
    "erlpack": "discord/erlpack",
    "ioredis": "4.17.3",
    "mangadex-full-api": "Mxblu/mangadex-full-api#order-optional",
    "ts-node": "^10.0.0",
    "utf-8-validate": "5.0.3",
    "zlib-sync": "0.1.7"
  },
  "devDependencies": {
    "@types/ioredis": "^4.26.4",
    "@types/node": "15.12.2",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "eslint": "7.28.0",
    "nodemon": "^2.0.9",
    "typescript": "4.3.2"
  }
}
